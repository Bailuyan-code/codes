void reverse(int*a,int left,int right)//逆置
{
    while(left<right)
    {
        int tmp=a[left];
        a[left]=a[right];
        a[right]=tmp;
        ++left;
        --right;
    }
}
void rotate(int* nums, int numsSize, int k){
if(k>numsSize)
k%=numsSize;
reverse(nums,0,numsSize-k-1);
reverse(nums,numsSize-k,numsSize-1);
reverse(nums,0,numsSize-1);

}
